    <section id="contact">
      <div class="container">
        <div class="row">
          <div class="col-lg-8 mx-auto text-center">
            <h3 class="section-heading">
              You are not alone! We are here for you
            </h3>
            <hr class="my-4" />
            <p class="mb-5">
              Ready to start your next project with us? That's great! Give us a
              call or send us an email and we will get back to you as soon as
              possible!
            </p>
          </div>
        </div>
        <div
          class="row no-gutters d-lg-flex justify-content-lg-center align-items-lg-center"
        >
          <div class="col-lg-4 text-center text-center">
            <i
              class="fa fa-envelope-o fa-3x mb-3 sr-contact"
              id="mail_about"
              data-aos="zoom-in"
              data-aos-duration="300"
              data-aos-delay="300"
              data-aos-once="true"
            ></i>
            <p>
              <a href="mailto:helpdesk@qolab.io">helpdesk@qolab.io</a>
            </p>
          </div>
        </div>
      </div>
    </section>
